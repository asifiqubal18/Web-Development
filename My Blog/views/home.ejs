<%- include('partials/header'); -%>
    <h1>Home</h1>
    <p>
        <%= StartingContent %>
    </p>
    <%for(var i=0;i<posts.length;i++){ %>
        <h1><%= posts[i].textTitle%></h1>
        <% if(posts[i].textPost.length>=100){%>
            <p><%= posts[i].textPost.substr(0,400)+ "..." %>
            <a href="/posts/<%=posts[i].textTitle %>">Read More</a>
            </p>
        <%}%>
       
     
    <% }%>
   
<%- include('partials/footer'); -%>